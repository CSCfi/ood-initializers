dashboard_layout:
  rows:
    - columns:
      - width: 12
        widgets:
          - shared_style
          - csc_pinned_apps
          - notifications
          - grafana
pinned_apps:
  - "sys/files"
  - "sys/ood-disk-quotas"
  - "sys/ood-persistent-ssh"
  - "sys/ood-shell"
  - "sys/ood-vnc"
  - "sys/ood-virtualgl"
  - "sys/ood-base-jupyter"
  - "sys/ood-course-jupyter"
  - "sys/ood-matlab-html"
  - "sys/ood-rstudio"
  - "sys/ood-tensorboard"
  - "sys/ood-vscode"
pinned_apps_menu_length: 0

brand_bg_color: "#FFFFFF"
brand_link_active_bg_color: "#D8E8EA"
navbar_type: "light"

help_bar:
  - "develop"
  - "help"
  - "user"
  - "log_out"

custom_css_files: ['../assets/stylesheets/dashboard.css']
cancel_session_enabled: true

<% if ["test", "staging", "future"].include?(ENV["SLURM_OOD_ENV"]) %>
custom_pages:
  versions:
    rows:
      - columns:
        - width: 12
          widgets:
            - shared_style
            - versions
<% end %>
