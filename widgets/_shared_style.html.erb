<script>
  (function (){
    if (!styleExists()) {
      addStyle();
    }
  })();
  function styleExists() {
    return $("#dashboard_style").length > 0;
  };
  function addStyle() {
    const style = document.createElement('style');
    style.type = 'text/css';
    style.id = "dashboard_style";
    style.textContent = `
      body {
        background-color: #d8e8ea;
      }
      .container-md {
        max-width: 1200px;
      }
      .row {
        margin: unset;
      }
      .col-md-12 {
        background-color: #ffffff;
        border-radius: 4px;
      }
      footer {
        background-color: #ffffff;
      }
      .dashboard-container {
        margin-bottom: 1rem;
        padding: 2rem;
      }
      .dashboard-container-title {
        margin-bottom: 1.5rem;
        flex-basis: 100%;
      }
      .dashboard-container-title-text {
        font-size: 25px;
        color: #000000;
        font-weight: 700;
        display: inline-block;
      }
      .dashboard-container-title-underline {
        width: 44px;
        height: 4px;
        border-radius: 4px;
        background-color: #006778;
        margin-top: 10px;
      }
    `;
    document.head.appendChild(style);
  }
</script>
